{
    "name": "jmleroux/enhanced-entity-bundle",
    "type": "symfony-bundle",
    "description": "PoC to add info to entity without override",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "JM Leroux <jean-marie.leroux@akeneo.com>"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git",
            "branch": "master"
        }
    ],
    "require": {
        "php": ">= 5.5",
        "akeneo/pim-community-dev": "^1.5.2",
        "doctrine/migrations": "1.0.0-alpha3@alpha"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "fabpot/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Akeneo\\Bundle\\EnhancedEntityBundle\\": ""
        }
    },
    "config": {
